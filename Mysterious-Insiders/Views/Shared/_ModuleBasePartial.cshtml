@model ModuleBase


<head>
	<style>
		.ModuleBase {
			position: absolute;
			border: 1px solid black;
			border-radius: 3px;
		}
	</style>
</head>

<div class="ModuleBase" style="left: @(Model.X + "px");
			top: @(Model.Y)px;
			width: @(Model.Width)px;
			height: @(Model.Height)px;
			color: @("#" + Model.TextColor.Name);
			">
	@*This is a module to display the @Model.Id;*@
	@Model.Text


</div>